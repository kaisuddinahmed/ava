{
  "name": "@ava/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "db:push": "DATABASE_URL=${DATABASE_URL:-file:./dev.db} prisma db push",
    "db:push:fast": "DATABASE_URL=${DATABASE_URL:-file:./dev.db} prisma db push --skip-generate",
    "db:generate": "DATABASE_URL=${DATABASE_URL:-file:./dev.db} prisma generate",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^6.3.0"
  },
  "devDependencies": {
    "prisma": "^6.3.0",
    "typescript": "^5.6.0"
  }
}
